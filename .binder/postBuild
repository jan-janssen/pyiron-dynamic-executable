# install environment 
mamba env update --name notebook --file environment.yml

# move pyiron config to home directory 
mv .binder/.pyiron .

# import dataset
bash .github/ci_support/import_dataset.sh

# install pyiron_base from Github
pip install git+https://github.com/pyiron/pyiron_base.git@remove_dynamic --no-deps --no-build-isolation
